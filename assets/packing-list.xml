<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:body>
    <w:p>
      <w:r>
        <w:t>Template Packing List</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Date: [Insert Date]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Order Number: [Insert Order Number]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Shipment Tracking Number: [Insert Tracking Number]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Shipping Method: [Insert Shipping Method]</w:t>
      </w:r>
    </w:p>
    <w:tbl>
      <w:tblPr>
        <w:tblBorders>
          <w:top w:val="single" w:sz="4" w:space="0" w:color="auto"/>
          <w:left w:val="single" w:sz="4" w:space="0" w:color="auto"/>
          <w:bottom w:val="single" w:sz="4" w:space="0" w:color="auto"/>
          <w:right w:val="single" w:sz="4" w:space="0" w:color="auto"/>
          <w:insideH w:val="single" w:sz="4" w:space="0" w:color="auto"/>
          <w:insideV w:val="single" w:sz="4" w:space="0" w:color="auto"/>
        </w:tblBorders>
      </w:tblPr>
      <w:tblGrid>
        <w:gridCol w:w="2000"/>
        <w:gridCol w:w="4000"/>
        <w:gridCol w:w="2000"/>
      </w:tblGrid>
      <w:tr>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>Item No.</w:t>
            </w:r>
          </w:p>
        </w:tc>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>Description</w:t>
            </w:r>
          </w:p>
        </w:tc>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>Quantity</w:t>
            </w:r>
          </w:p>
        </w:tc>
      </w:tr>
      <w:tr>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>[001]</w:t>
            </w:r>
          </w:p>
        </w:tc>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>[Product Name]</w:t>
            </w:r>
          </w:p>
        </w:tc>
        <w:tc>
          <w:p>
            <w:r>
              <w:t>[Qty]</w:t>
            </w:r>
          </w:p>
        </w:tc>
      </w:tr>
    </w:tbl>
    <w:p>
      <w:r>
        <w:t>Subtotal: [Insert Subtotal]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Shipping Costs: [Insert Shipping Costs]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Tax: [Insert Tax]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Total Amount: [Insert Total]</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:r>
        <w:t>Special Instructions: [Insert Instructions]</w:t>
      </w:r>
    </w:p>
  </w:body>
</w:document>
